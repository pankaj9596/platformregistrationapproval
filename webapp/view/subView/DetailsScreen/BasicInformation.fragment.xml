        <layout:Grid defaultSpan="XL12 L12 M12 S12">
                <m:VBox>
                    <m:Label text="{i18n>EmailID}" design="Bold" />
                    <m:Text text="{oFiexibleLayout>/retailerReg/EMAIL_ID}" />
                    <m:layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" linebreak="true" />
                    </m:layoutData>
                </m:VBox>
                <m:VBox>
                    <m:Label text="{i18n>Title}" design="Bold" />
                    <m:Text text="{oFiexibleLayout>/retailerReg/TITLESEQID}" />
                    <m:layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" linebreak="true" />
                    </m:layoutData>
                </m:VBox>
                <m:VBox>
                    <m:Label text="{i18n>FirstName}" design="Bold" />
                    <m:Text text="{oFiexibleLayout>/retailerReg/FIRST_NAME}" />
                    <m:layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" linebreak="true" />
                    </m:layoutData>
                </m:VBox>

                <m:VBox>
                    <m:Label text="{i18n>MiddleName}" design="Bold" />
                    <m:Text text="{oFiexibleLayout>/retailerReg/MIDDLE_NAME}" />
                    <m:layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" />
                    </m:layoutData>
                </m:VBox>
                <m:VBox>
                    <m:Label text="{i18n>LastName}" design="Bold" />
                    <m:Text text="{oFiexibleLayout>/retailerReg/LAST_NAME}" />
                    <m:layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" />
                    </m:layoutData>
                </m:VBox>
                <m:VBox>
                    <m:Label text="{i18n>RegistrationDate}" design="Bold" />
                    <m:Text text="{path:'oFiexibleLayout>/retailerReg/REGISTRATION_DATE',type:'sap.ui.model.type.Date',formatOptions:{strictParsing:true,UTC:true,patter:'dd/MM/yyyy'}}" />
                    <m:layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" />
                    </m:layoutData>
                </m:VBox>
     </layout:Grid>
